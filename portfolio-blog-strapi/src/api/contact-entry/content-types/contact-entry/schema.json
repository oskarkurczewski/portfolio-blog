{
  "kind": "collectionType",
  "collectionName": "contact_entries",
  "info": {
    "singularName": "contact-entry",
    "pluralName": "contact-entries",
    "displayName": "contact-entry",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "surname": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true,
      "regex": "^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$"
    },
    "message": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "subject": {
      "type": "string",
      "required": true
    }
  }
}
