{
  "kind": "collectionType",
  "collectionName": "contact_entries",
  "info": {
    "singularName": "contact-entry",
    "pluralName": "contact-entries",
    "displayName": "contact-entry"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Surname": {
      "type": "string",
      "required": true
    },
    "Email": {
      "type": "string",
      "required": true,
      "regex": "^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$"
    },
    "Message": {
      "type": "text",
      "required": true,
      "unique": true
    }
  }
}
